! clear mod4
keysym Alt_R = Mode_switch
keycode 10 = 1 exclam exclamdown degree
keycode 11 = 2 at twosuperior
keycode 19 = 0 parenright degree
keycode 21 = equal plus masculine ordfeminine
keycode 26 = e E EuroSign
keycode 30 = u U udiaeresis Udiaeresis
keycode 32 = o O odiaeresis Odiaeresis
keycode 38 = a A adiaeresis Adiaeresis
keycode 39 = s S ssharp
keycode 40 = d D dstroke Dstroke
keycode 48 = dead_acute quotedbl apostrophe
keycode 49 = grave asciitilde dead_grave ordfeminine
keycode 54 = c C cacute Cacute
keycode 55 = v V dead_caron
keycode 57 = n N ntilde Ntilde
keycode 61 = slash question questiondown
keycode 166 = Prior
keycode 167 = Next
! Mult_key in Scroll Lock
keycode 78 = Multi_key
add mod1 = Meta_L
! add mod3 = Super_L Super_R
! The menu key is mapped to "Menu", but we want to avoid the applications to use it
keysym Menu = F13
add mod3 = F13
remove Lock = Caps_Lock
keysym Caps_Lock = Escape
add Lock = Print
